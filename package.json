{
  "name": "webpack-aot",
  "version": "1.0.0",
  "description": "Angular 2 AOT (Ahead Of Time) offline compilation example with Webpack",
  "main": "index.js",
  "scripts": {
    "ngc": "ngc -p ./tsconfig.json",
    "serve": "http-server ./dist -a 0.0.0.0 -p 9000",
    "start-prod": "npm run ngc && npm run webpack-prod && npm run serve",
    "webpack-prod": "webpack --config webpack.prod.config.js"
  },
  "keywords": [
    "angular2",
    "aot",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniele-zurico/webpack-aot.git"
  },
  "author": {
    "name": "daniele zurico",
    "email": "daniele.zurico@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "^2.2.1",
    "@angular/http": "^2.2.1",
    "@angular/material": "^2.0.0-alpha.10",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "core-js": "2.4.1",
    "rxjs": "5.0.0-rc.4",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@ngtools/webpack": "1.1.7",
    "@types/hammerjs": "^2.0.33",
    "@types/node": "6.0.48",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "chokidar": "1.6.1",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "gh-pages": "0.12.0",
    "http-server": "0.9.0",
    "raw-loader": "0.5.1",
    "sass-loader": "^4.0.2",
    "source-map-explorer": "1.3.3",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
